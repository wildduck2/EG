{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/service-worker.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/service-worker.js"
        }
    ]
}
